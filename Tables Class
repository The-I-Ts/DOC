package CMPS280;

import java.util.Scanner;

public abstract class Tables extends Orders { 
	
	public static int TableNum;
	
	public Tables() {}
	
	Tables(int TableNum) {
		Tables.TableNum = TableNum; }
	
	public void setTableNum(int TableNumber) {
		TableNum = TableNumber; }
	
	public void getTableNum() {
	    return; }
}
